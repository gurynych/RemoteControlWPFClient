<Window x:Class="RemoteControlWPFClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RemoteControlWPFClient"
        xmlns:dp="clr-namespace:RemoteControlWPFClient.MVVM.AttachedProperties"
        xmlns:dxmvvm="http://schemas.devexpress.com/winfx/2008/xaml/mvvm"
        mc:Ignorable="d"
        Title="MainWindow"
        Height="450"
        Width="800"
        DataContext="{Binding MainViewModel, Source={StaticResource vmLocator}}">
    <dxmvvm:Interaction.Behaviors>
        <dxmvvm:EventToCommand EventName="Loaded"
                               Command="{Binding MainWindowLoadedCommand}" />
    </dxmvvm:Interaction.Behaviors>
    <Grid>
    </Grid>
</Window>
